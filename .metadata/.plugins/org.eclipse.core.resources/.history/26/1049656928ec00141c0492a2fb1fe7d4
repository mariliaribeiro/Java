import java.util.ArrayList;
import java.util.List;


public class ClienteContas extends Cliente{
	protected List clientes;
		
	public ClienteContas(String nome, String cpf){
		super(nome, cpf);
		this.cliente = cliente;
		this.conta = conta;
		this.clientes = new ArrayList<Cliente>();
		this.contas = new ArrayList<CCorrente>();
	}
	
	public void associaClienteConta(Cliente cliente, CCorrente conta){
		this.clientes.add(new Cliente(nome, cpf));
		//Cliente clinte = clientes.get(i);
	}
	
	public void addConta(CCorrente conta){
		this.contas.add(conta);
	}
	
	public List getContas(){
		return contas;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return clientes.to;
	}
}
