import java.util.ArrayList;
import java.util.List;


public class ClienteContas extends Cliente{
	protected List clientes;
		
	public ClienteContas(String nome, String cpf){
		super(nome, cpf);
		this.clientes = new ArrayList<Cliente>();
	}
	
	public void associaClienteConta(Cliente cliente, CCorrente conta){
		this.clientes.add(new Cliente(nome, cpf));
	}
	
	public List getClientes(){
		return clientes;
	}
	
	
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		Cliente cliente = (Cliente) clientes.get(1);
		return cliente.toString();
	}
}
