import java.util.ArrayList;
import java.util.List;


public class ClienteContas{
	protected List clientes;
	List contas;
	Cliente cliente;
	CCorrente conta;
	
	public ClienteContas(Cliente cliente, CCorrente conta){
		this.cliente = cliente;
		this.conta = conta;
		this.clientes = new ArrayList<Cliente>();
		this.contas = new ArrayList<CCorrente>();
	}
	
	public void associaClienteConta(Cliente cliente, CCorrente conta){
		this.clientes.add(cliente);
	}
	
	public void addConta(CCorrente conta){
		this.contas.add(conta);
	}
	
	public List getContas(){
		return contas;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return clientes.toString()/*cliente.getContas().toString()*/;
	}
}
